@article{ghosh_probabilistic_2006,
	title = {On probabilistic properties of conditional medians and quantiles},
	volume = {76},
	issn = {0167-7152},
	url = {http://www.sciencedirect.com/science/article/pii/S0167715206001416},
	doi = {10.1016/j.spl.2006.04.024},
	abstract = {Conditional medians and quantiles are frequently used in analyzing time series data with heavy tails for their robustness properties. Most of the available literature focusses on statistical estimation of conditional quantiles and large sample behavior of the proposed estimators, whereas probabilistic properties of the true conditional medians themselves have not been fully explored (Tomkins [1975. On conditional medians. Ann. Probab. 3, 375–379; 1978. Convergence properties of conditional medians. Canad. J. Statist. 6, 169–177]). It is well-known that not all properties of conditional expectations have analogues for conditional medians. In this short note, we study to what extent analogues of certain properties of conditional expectations hold for conditional medians and generalize some of these properties to any general conditional quantile.},
	number = {16},
	urldate = {2014-02-06},
	journal = {Statistics \& Probability Letters},
	author = {Ghosh, Yashowanto N. and Mukherjee, Bhramar},
	month = oct,
	year = {2006},
	keywords = {Conditional expectation, Conditional quantile, Conditional variance, Mean absolute deviation},
	pages = {1775--1780},
	file = {Ghosh_Mukherjee_2006_On probabilistic properties of conditional medians and quantiles.pdf:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/PBUXNINV/Ghosh_Mukherjee_2006_On probabilistic properties of conditional medians and quantiles.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/IEUMDZAF/S0167715206001416.html:text/html}
