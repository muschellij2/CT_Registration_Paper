@article{doksum_spline_2000,
	title = {On spline estimators and prediction intervals in nonparametric regression},
	volume = {35},
	issn = {0167-9473},
	url = {http://www.sciencedirect.com/science/article/pii/S0167947399001164},
	doi = {10.1016/S0167-9473(99)00116-4},
	abstract = {The quantile regression function gives the quantile in the conditional distribution of a response variable given the value of a covariate. It can be used to measure the effect of covariates not only in the center of a population, but also in the upper and lower tails. Moreover, it provides prediction intervals that do not rely on normality or other distributional assumptions. In a nonparametric setting, we explore a class of quantile regression spline estimators of the quantile regression function. We consider an automatic knot selection procedure involving a linear programming method, stepwise knot addition using a modified {AIC}, and stepwise knot deletion using a modified {BIC.} Because the methods estimate quantile regression functions, they possess an inherent robustness to extreme observations in the response values. We investigate the performance of prediction intervals based on automatic quantile regression splines and find that the loss of efficiency of this procedure is minimal in the normal linear homoscedastic model. In heteroscedastic linear models, it outperforms the classical normal theory prediction interval. A data example is provided to illustrate the use of the proposed methods.},
	number = {1},
	urldate = {2014-02-06},
	journal = {Computational Statistics \& Data Analysis},
	author = {Doksum, Kjell and Koo, Ja-Yong},
	month = nov,
	year = {2000},
	keywords = {{AIC}, {BIC}, Information criterion, Knot selection, Polynomial splines, Quantile regression function, Stepwise knot addition, Stepwise knot deletion},
	pages = {67--82},
	file = {Doksum_Koo_2000_On spline estimators and prediction intervals in nonparametric regression.pdf:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/CH8WE76U/Doksum_Koo_2000_On spline estimators and prediction intervals in nonparametric regression.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/HMJV973C/S0167947399001164.html:text/html}
},

